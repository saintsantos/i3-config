{
  "name": "source-preview",
  "main": "./lib/main",
  "version": "0.5.0",
  "description": "Source Preview for Atom",
  "keywords": [
    "coffeescript",
    "preview"
  ],
  "activationCommands": {
    "atom-text-editor[data-grammar~='source']": [
      "source-preview:toggle"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aki77/atom-source-preview.git"
